{
  "name": "shark-admin-pro",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite serve --mode development",
    "build:prod": "vite build --mode production && vue-tsc --noEmit",
    "lint:eslint": "eslint  --fix --ext .ts,.js,.vue ./src ",
    "lint:prettier": "prettier --write \"**/*.{js,cjs,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint  \"**/*.{css,scss,vue}\" --fix",
    "lint:lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{cjs,json}": [
      "prettier --write"
    ],
    "*.{vue,html}": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,css}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@iconify/vue": "^4.1.2",
    "@sk/color-preset": "workspace:*",
    "@vueuse/core": "^11.0.1",
    "ant-design-vue": "4.x",
    "axios": "^1.7.4",
    "axios-retry": "^4.5.0",
    "clipboard": "^2.0.11",
    "colord": "2.9.3",
    "dayjs": "^1.11.13",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.0.7",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.2",
    "vue": "^3.4.37",
    "vue-i18n": "^9.14.0",
    "vue-router": "^4.4.3",
    "xlsx-js-style": "^1.2.0"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.242",
    "@sk/uno-preset": "workspace:*",
    "@types/node": "^22.4.1",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.15",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "@unocss/core": "^0.62.3",
    "@unocss/eslint-config": "^0.62.3",
    "@unocss/preset-icons": "^0.62.3",
    "@unocss/preset-uno": "^0.62.3",
    "@unocss/transformer-directives": "^0.62.3",
    "@unocss/transformer-variant-group": "^0.62.3",
    "@unocss/vite": "^0.62.3",
    "@vitejs/plugin-legacy": "^5.4.2",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.27.0",
    "lint-staged": "^15.2.9",
    "postcss": "^8.4.41",
    "postcss-html": "^1.7.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "stylelint": "^16.9.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^5.1.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "terser": "^5.31.6",
    "typescript": "^5.5.3",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-icons": "^0.19.2",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.1",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^2.0.29"
  },
  "description": "A mid to backend template based on Vue3, Vite5, TypeScript, Antd vue, and UnoCSS technology stacks.",
  "keywords": [
    "Vue3 admin ",
    "vue-admin-template",
    "Vite5",
    "TypeScript",
    "ant-design-vue v4",
    "UnoCSS"
  ],
  "author": {
